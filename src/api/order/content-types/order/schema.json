{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "processing",
        "shipped",
        "delivered",
        "cancelled",
        "onhold",
        "refunded",
        "failed",
        "returned",
        "awaitingPayment",
        "completed"
      ],
      "required": true,
      "default": "pending"
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    }
  }
}
